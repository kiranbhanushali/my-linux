Config { 
font         = "xft:SF ProText Regular:size=12:antialias=true:hinting=true"
        ,additionalFonts = ["xft:FontAwesome-9"]
        , bgColor = "white"
       , fgColor = "black"
       , lowerOnStart = True
       , persistent   = True
       , commands     = [ 
            Run MultiCpu [
                "--template" , "CPU:<total0>%|<total1>%|<total2>%|<total3>%"
                , "--Low"      , "30"         -- units: %
                , "--High"     , "85"         -- units: %
                , "--low"      , "green"
                , "--normal"   , "orange"
                , "--high"     , "red"
            ] 10,
            Run MultiCoreTemp       [ "--template" , "Temp: <avg>Â°C "
                ,"-L", "60", "-H", "80",
                "-l", "green", "-n", "yellow", "-h", "red",
                "--", "--mintemp", "20", "--maxtemp", "100"] 50
            ,Run Memory[
                "-t", "MEM:<usedratio>%"
                , "-p", "2"
                , "-l", "green"
                , "-h", "orange" -- blue, just to differentiate from cpu bar
            ] 10
            , Run Network "wlp5s0" [ "--template", "Wifi: <rx>-up <tx>-down"
            , "-S",         "true"
            ] 20
            , Run Date "<fc=#222622>%A, %B %d</fc> | <fc=#222622>%I:%M:%S %p</fc>" "date" 10
            , Run Battery
            [
                "-t", "BAT: <acstatus>"
                ,"-L", "30"
                , "-H", "85"
                , "-l", "red"
                , "-n", "orange"
                , "-h", "green"
                -- will notify 
                ,"-a","notify-send -u critical 'Battery running out!!'"
                , "--" -- battery specific options
                -- discharging status
                , "-o"  , "<fn=1>\xf242</fn> <left>%"
                -- AC "on" status
                , "-O"  , "<fn=1>\xf1e6</fn> <left>%"
                -- charged status
                , "-i"  , "<fn=1>\xf1e6</fn> <left>%"
                , "--off-icon-pattern", "<fn=1>\xf1e6</fn>"
                , "--on-icon-pattern", "<fn=1>\xf1e6</fn>"
            ] 10
            , Run UnsafeStdinReader
                ]
                , sepChar      = "%"
                , alignSep     = "}{"
                , template     = " %UnsafeStdinReader% }{  <fc=#222622>%memory%</fc>   <fc=#222622>%multicpu% %multicoretemp% </fc> <fc=#222622>%battery% </fc>  %date%                           <fc=darkred>K</fc><fc=black>iran</fc> <fc=darkred>B</fc><fc=black>hanushali</fc>"
}
